<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/setting_content_padlr"
    android:paddingRight="@dimen/setting_content_padlr"
    android:paddingTop="@dimen/setting_content_padtb"
    android:paddingBottom="@dimen/setting_content_padtb"
    android:orientation="vertical" >
    
	<!-- Header -->
   	<LinearLayout
        android:id="@+id/title"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:orientation="horizontal" >
    	
		 <TextView
            android:id="@+id/app_text"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/viewpager_icon_h"
            android:drawableLeft="@drawable/settings_icon"
            android:focusable="false"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/viewpager_left"
            android:text="@string/app_name"
            android:textColor="@android:color/white"
            android:textSize="30sp" >
        </TextView>

        <ImageView
            android:id="@+id/title_img"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:src="@drawable/arrow_right" />
        
    	<TextView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/storeinfo"
        android:textSize="@dimen/setting_title_size"
        android:gravity="center_vertical"
        android:textColor="@android:color/white"/>

    </LinearLayout>
     <TextView 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/settings_head_line"/>  
	    	
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="0dp"
		android:layout_weight="6"
		android:orientation="vertical"
		android:paddingTop="@dimen/viewpager_icon_textsize"
		android:gravity="center_horizontal">
   <!--  --> 	
    <LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:layout_marginLeft="50dp"
		android:layout_marginRight="50dp"
		android:visibility="gone"
		android:gravity="center">
	 <ImageView
	    android:id="@+id/imageView1"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_gravity="center_vertical"
	    android:visibility="gone"
	    android:src="@drawable/storage0" />
	
	<TextView android:id="@+id/sdcard"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:layout_gravity="center_vertical"
			android:paddingLeft="@dimen/setting_list_pad"
			android:textColor="#ffffffff"
			android:textSize="40dp"
			android:visibility="gone"
			android:text="@string/SDstore"/>
	</LinearLayout>
	
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:layout_marginLeft="150dp"
		android:visibility="gone"
		android:layout_marginRight="150dp"
		android:gravity="center">
		<TextView android:id="@+id/sdcard_total_title"
			android:layout_width="0dp"
			android:layout_height="wrap_content"   
			android:layout_weight="1"
			android:text="@string/SDallspace"
			android:textColor="#FFFFFFFF"
			android:visibility="gone"
			android:textSize="30dp"/>
		<TextView android:id="@+id/sdcard_total"
			android:layout_width="0dp"
			android:layout_height="wrap_content"   
			android:layout_weight="1"
			android:gravity="right"
			android:visibility="gone"
			android:textColor="#FFFFFFFF"
			android:textSize="30dp"/>
	</LinearLayout>

	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="150dp"
		android:layout_marginRight="150dp"
		android:orientation="horizontal"
		android:visibility="gone"
		android:gravity="center">
		<TextView android:id="@+id/sdcard_available_title"
			android:layout_width="0dp"
			android:layout_height="wrap_content"   
			android:layout_weight="1"
			android:text="@string/SDablespace"
			android:textColor="#FFFFFFFF"
			android:visibility="gone"
			android:textSize="30dp"/>
		<TextView android:id="@+id/sdcard_available"
			android:layout_width="0dp"
			android:layout_height="wrap_content"   
			android:layout_weight="1"
			android:visibility="gone"
			android:gravity="right"
			android:textColor="#FFFFFFFF"
			android:textSize="30dp"/>
	</LinearLayout>
	
	    <ImageView
	        android:id="@+id/imageView_line0"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_gravity="center"
	        android:visibility="gone"
	        android:layout_marginTop="15dip"
	        android:src="@drawable/setting_listview_split" />	
	        
    <LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:layout_marginLeft="50dp"
		android:layout_marginRight="50dp"
		android:paddingTop="@dimen/viewpager_icon_textsize"
		android:gravity="center">
	    
	 <ImageView
	    android:id="@+id/imageView2"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_gravity="center_vertical"
	    android:src="@drawable/storage2" />

	<TextView android:id="@+id/nandflash"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:layout_gravity="center_vertical"
			android:layout_marginTop="7dip"
			android:paddingLeft="@dimen/setting_list_pad"
			android:textColor="#ffffffff"
			android:textSize="40dp"
			android:text="@string/NandFlash"/>
	</LinearLayout>
	
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="150dp"
		android:layout_marginRight="150dp"
		android:orientation="horizontal"
		android:gravity="center">
		<TextView android:id="@+id/nand_total_title" 
			android:layout_width="0dp"
			android:layout_height="wrap_content"   
			android:layout_weight="1"
			android:layout_marginTop="7dip"
			android:text="@string/Nandallspace"
			android:textColor="#FFFFFFFF"
			android:textSize="30dp"/>
		<TextView android:id="@+id/nand_total"
			android:layout_width="0dp"
			android:layout_height="wrap_content"   
			android:layout_weight="1"
			android:gravity="right"
			android:text="@string/Nandallspace"
			android:textColor="#FFFFFFFF"
			android:textSize="30dp"/>
	</LinearLayout>
<!--  
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="150dp"
		android:layout_marginRight="150dp"
		android:orientation="horizontal"
		android:gravity="center">
		<TextView android:id="@+id/nand_available_title"
			android:layout_width="0dp"
			android:layout_height="wrap_content"   
			android:layout_weight="1"
			android:layout_marginTop="7dip"
			android:text="@string/Nandablespace"
			android:textColor="#FFFFFFFF"
			android:textSize="30dp"/>
		<TextView android:id="@+id/nand_available"
			android:layout_width="0dp"
			android:layout_height="wrap_content"   
			android:layout_weight="1"
			android:gravity="right"
			android:textColor="#FFFFFFFF"
			android:textSize="25dp"/>
	</LinearLayout>
-->
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="150dp"
		android:layout_marginRight="150dp"
		android:orientation="horizontal"
		android:gravity="center">
		<TextView android:id="@+id/nand_availabletotal_title"
			android:layout_width="0dp"
			android:layout_height="wrap_content"   
			android:layout_weight="1"
			android:layout_marginTop="7dip"
			android:text="@string/Nandablespacetotal"
			android:textColor="#FFFFFFFF"
			android:textSize="30dp"/>
		<TextView android:id="@+id/nand_availabletotal"
			android:layout_width="0dp"
			android:layout_height="wrap_content"   
			android:layout_weight="1"
			android:gravity="right"
			android:textColor="#FFFFFFFF"
			android:textSize="30dp"/>
	</LinearLayout>
	</LinearLayout>

</LinearLayout>
